{
  manifest_version: "0.1.0",
  program: {
    image: "docker.io/litellm/litellm:latest",
    entrypoint: ["litellm", "--model", "${config.model}"],
    network: {
      endpoints: [
        { name: "http", port: 80 },
      ],
    },
  },
  config_schema: {
    type: "object",
    properties: {
      model: { type: "string" },
    },
    required: ["model"],
    additionalProperties: false,
  },
  components: {
    proxy: "./litellm-proxy.json5",
    wrapper: "./litellm-wrapper.json5",
  },
  provides: {
    http: { kind: "http", endpoint: "http" },
  },
  bindings: [
    { to: "#proxy.backend", from: "self.http" },
    { to: "#wrapper.litellm", from: "#proxy.admin_api" },
  ],
  exports: { llm: "#proxy.llm", admin_api: "#wrapper.admin_api" },
}
