{
  manifest_version: "0.1.0",
  program: {
    image: "ghcr.io/agentbeats/tau2-agent:v1.0.0",
    args: "--llm-api ${slots.llm.url} --port 443",
    network: {
      endpoints: [
        { name: "endpoint", port: 443, path: "/" },
      ],
    },
  },
  slots: {
    llm: { kind: "llm" },
  },
  provides: {
    a2a: { kind: "a2a" },
  },
  exports: { llm: "llm", a2a: "a2a" },
}
