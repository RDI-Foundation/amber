{
  manifest_version: "0.1.0",
  program: {
    image: "ghcr.io/sierra-research/tau2:v2",
    entrypoint: ["tau2"],
    network: {
      endpoints: [
        { name: "endpoint", port: 8080 },
      ],
    },
  },
  provides: {
    env: { kind: "mcp", profile: "openenv", endpoint: "endpoint" },
  },
  exports: { env: "env" },
}
