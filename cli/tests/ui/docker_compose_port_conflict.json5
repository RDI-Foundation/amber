{
  manifest_version: "0.1.0",
  program: {
    image: "alpine:3.20",
    network: {
      endpoints: [
        { name: "api", port: 8080 },
        { name: "admin", port: 8080 },
      ],
    },
  },
  provides: {
    api: { kind: "http", endpoint: "api" },
    admin: { kind: "http", endpoint: "admin" },
  },
  exports: { api: "api", admin: "admin" },
}
