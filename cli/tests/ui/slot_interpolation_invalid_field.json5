{
  manifest_version: "0.1.0",
  program: {
    image: "ghcr.io/acme/app:v1",
    entrypoint: [
      "app",
      "--agent",
      "${slots.agent.blabla}",
    ],
    network: {
      endpoints: [
        { name: "agent", port: 8080 },
      ],
    },
  },
  slots: {
    agent: { kind: "a2a", optional: true },
  },
  provides: {
    agent_provider: { kind: "a2a", endpoint: "agent" },
  },
  exports: {
    agent: "agent_provider",
  },
}
