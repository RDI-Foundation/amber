{
  manifest_version: "0.1.0",
  program: {
    image: "ghcr.io/acme/app:v1",
    entrypoint: [
      "app",
      "--backend",
      "${bindings.route.url}",
    ],
    network: {
      endpoints: [
        { name: "http", port: 8080 },
      ],
    },
  },
  components: {
    backend: "./binding_interpolation_unknown_binding_child.json5",
  },
  provides: {
    api: { kind: "http", endpoint: "http" },
  },
  bindings: [
    { name: "backend", to: "#backend.backend", from: "self.api" },
  ],
}
